<div class="container">
  <h1>Organization ign In</h1>

  <div class="col-lg-4">
    <%= form_tag orgsessions_path do %>
    <div class="form-group">
      <%= label_tag :email %>
      <%= text_field_tag :email, '', class:'form-control' %>
    </div>
    <div class="form-group">
      <%= label_tag :password %>
      <%= password_field_tag  :password, '', class:'form-control' %>
    </div>
      <%= submit_tag 'Sign In', class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
